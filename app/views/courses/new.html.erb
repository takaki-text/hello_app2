<h1>Courses#new</h1>
<p>Find me in app/views/courses/new.html.erb</p>

<%= form_for @course do |f| %>
  <p>
    <%= f.label :plan,"新サービス登録" %>
    <%= f.text_field :plan %>
  </p>
  <div class="field form-group form-group-lg">
  <%= f.label :tag_list,"tag登録" ,class: "sr-only control-label" %>
  <%= text_field_tag 'course[tag_list]', @course.tag_list.join(","), class: "form-control" %>
</div>

  <%= f.submit %>
<% end %>