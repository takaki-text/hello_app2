
<h1>Courses#index</h1>
<p>Find me in app/views/courses/index.html.erb</p><%= link_to "トップページへ戻る", root_path %>
<% @courses.reverse_each do |course| %>
 <p><span>name: </span><%=link_to course.user.name, "/courses/#{course.id}"%></p>
  <p><span>サービスの内容: </span><%=course.plan%></p>
  <span>tag: </span><% course.tag_list.each do |tag| %>
  <span class="badge badge-primary"><%= tag %></span>
<% end %>


<% end %>

<%= link_to "トップページへ戻る", root_path %>